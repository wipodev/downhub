{
  "manifest_version": 3,
  "name": "DownTube",
  "version": "0.0.1",
  "description": "Extension to download YouTube videos using a local backend.",
  "permissions": ["activeTab"],
  "host_permissions": ["https://www.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
